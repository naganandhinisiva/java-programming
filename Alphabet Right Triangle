import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n=sc.nextInt();
        for (int i=1;i<=n;i++) {
            char start=(char)('A' + i - 1);
            for (int j=0;j<i;j++) {
                System.out.print((char)(start + j));
                if (j<i-1) System.out.print(" ");
            }
            System.out.println();
        }
    }
}
